#include <stdio.h>

int main() {
    
    int a,b,c;
    printf("type the numbers");
    scanf ("%d%d%d", &a,&b,&c);
    
    if (a<b && a<c)
    {
        printf("a is the smallest");
    }
    
    else if (b<a && b<c)
    {
        printf("b is the smallest");
    }
    
    else if (c<a && c<b)
    {
        printf("c is the smallest");
    }
    return 0;
}
