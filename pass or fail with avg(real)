#include <stdio.h>

int main() {
   int a,b,c,d,e,f;
   printf("enter the marks of all the subjects");
   scanf("%d%d%d%d%d", &a,&b,&c,&d,&e);
   
   if(a>40 && b>40 && c>40 && d>40 && e>40)
   {
       printf("Student is pass");
   }
   
   else
   {
       printf("Student is fail");
   }
   
   f=(a+b+c+d+e)/5;
   printf("Average=%d",f);
   
       return 0;
}
