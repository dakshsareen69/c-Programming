#include <stdio.h>

int main() {
    
    int a,b,c,d;
    printf("type the numbers");
    scanf ("%d%d%d%d", &a,&b,&c,&d);
    
    if (a>b && a>c && a>d)
    {
        printf("a is the biggest");
    }
    
    else if (b>a && b>c && b>d)
    {
        printf("b is the biggest");
    }
    
    else if (c>a && c>b && c>d)
    {
        printf("c is the biggest");
    }
    
    else if (d>a && d>b && d>c)
    {
        printf("d is the biggest");
    }
    return 0;
}
